{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "entryFile": "src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "./",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": [
          "proto/user.proto"
        ],
        "watchAssets": true
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json",
        "assets": [
          "proto/chat.proto"
        ],
        "watchAssets": true
      }
    },
    "channel": {
      "type": "application",
      "root": "apps/channel",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/channel/tsconfig.app.json",
        "assets": [
          "proto/channel.proto"
        ],
        "watchAssets": true
      }
    },
    "thread": {
      "type": "application",
      "root": "apps/thread",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/thread/tsconfig.app.json",
        "assets": [
          "proto/thread.proto"
        ],
        "watchAssets": true
      }
    },
    "upload": {
      "type": "application",
      "root": "apps/upload",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/upload/tsconfig.app.json",
        "assets": [
          "proto/upload.proto"
        ],
        "watchAssets": true
      }
    },
    "rabbitmq": {
      "type": "library",
      "root": "libs/rabbitmq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq/tsconfig.lib.json"
      }
    }
  }
}